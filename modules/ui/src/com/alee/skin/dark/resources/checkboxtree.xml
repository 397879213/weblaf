<skin xmlns="http://weblookandfeel.com">

    <!-- com.alee.extended.tree.WebCheckBoxTree -->

    <!-- Checkbox tree -->
    <style type="tree" id="checkboxtree">

        <!-- Tristate checkbox cell renderer -->
        <style type="tristatecheckbox" id="renderer" padding="0,4,0,0" />

    </style>

    <!-- Transparent checkbox tree -->
    <style type="tree" id="checkboxtree-transparent" extends="checkboxtree">
        <component>
            <opaque>false</opaque>
        </component>
        <painter>
            <decorations overwrite="true" />
        </painter>
    </style>

</skin>