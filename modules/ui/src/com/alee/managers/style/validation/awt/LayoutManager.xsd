<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema targetNamespace="http://weblookandfeel.com" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

    <xs:include schemaLocation="../lang/Class.xsd" />

    <!-- java.awt.LayoutManager -->
    <xs:complexType name="LayoutManager">

        <!-- Any custom layout settings -->
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:any processContents="skip" />
        </xs:choice>

        <!-- Layout implementation class -->
        <xs:attribute name="class" type="Class" />

        <!-- Any custom attribute -->
        <xs:anyAttribute />

    </xs:complexType>

</xs:schema>