<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema targetNamespace="http://weblookandfeel.com/icons" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

    <!-- java.awt.Dimension -->
    <xs:simpleType name="Dimension">
        <xs:restriction base="xs:string">

            <!-- Integer width and height values -->
            <xs:pattern value="^[0-9]+,[0-9]+$" />

        </xs:restriction>
    </xs:simpleType>

</xs:schema>