<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema targetNamespace="http://weblookandfeel.com/icons" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

    <xs:include schemaLocation="AbstractSvgAttributeAdjustment.xsd" />
    <xs:include schemaLocation="../../awt/Color.xsd" />

    <!-- com.alee.extended.svg.SvgStroke -->
    <xs:complexType name="SvgStroke">
        <xs:complexContent>
            <xs:extension base="AbstractSvgAttributeAdjustment">

                <!-- Stroke color -->
                <xs:attribute name="color" type="Color" use="optional" />

            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

</xs:schema>