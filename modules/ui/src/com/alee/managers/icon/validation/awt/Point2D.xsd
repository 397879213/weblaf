<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema targetNamespace="http://weblookandfeel.com/icons" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

    <!-- java.awt.geom.Point2D -->
    <xs:simpleType name="Point2D">
        <xs:restriction base="xs:string">

            <!-- Float X,Y values -->
            <xs:pattern value="^[+-]?([0-9]*[.])?[0-9]+,[+-]?([0-9]*[.])?[0-9]+$" />

        </xs:restriction>
    </xs:simpleType>

</xs:schema>